{
  "name": "@tytan-auth/core",
  "version": "0.0.66",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "description": "",
  "type": "module",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "chokidar \"src/**/*.ts\" -c \"pnpm dev.build\" --initial",
    "dev.build": "pkgroll --clean-dist --sourcemap=inline && npx pkg-to-dist",
    "build": "pkgroll --clean-dist --minify"
  },
  "devDependencies": {
    "@binochoi/eslint-config-typescript": "^0.0.25",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^6.21.0",
    "chokidar-cli": "^3.0.0",
    "eslint": "^9.0.0",
    "npm-run-all": "^4.1.5"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      }
    },
    "./jwt": {
      "require": {
        "types": "./dist/jwt.d.ts",
        "default": "./dist/jwt.cjs"
      },
      "import": {
        "types": "./dist/jwt.d.ts",
        "default": "./dist/jwt.mjs"
      }
    },
    "./oauth": {
      "require": {
        "types": "./dist/oauth.d.ts",
        "default": "./dist/oauth.cjs"
      },
      "import": {
        "types": "./dist/oauth.d.ts",
        "default": "./dist/oauth.mjs"
      }
    }
  },
  "keywords": [],
  "dependencies": {
    "@types/jsonwebtoken": "^9.0.6",
    "@tytan-auth/common": "../common/dist",
    "@tytan-auth/provider": "../provider",
    "arctic": "^1.9.2",
    "jose": "^5.9.4",
    "jsonwebtoken": "^9.0.2",
    "nanoid": "^3.3.7",
    "pkgroll": "^2.4.2",
    "typescript": "^5.5.4"
  }
}
