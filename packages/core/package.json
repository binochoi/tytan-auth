{
  "name": "@tytan-auth/core",
  "version": "0.0.4",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "description": "",
  "type": "module",
  "files": [
    "dist"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "pkgroll --clean-dist --sourcemap=inline --watch",
    "build": "pkgroll --clean-dist --minify"
  },
  "devDependencies": {
    "@binochoi/eslint-config-typescript": "^0.0.25",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^9.0.0"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      }
    },
    "./jwt": {
      "require": {
        "types": "./dist/jwt.d.ts",
        "default": "./dist/jwt.cjs"
      },
      "import": {
        "types": "./dist/jwt.d.ts",
        "default": "./dist/jwt.mjs"
      }
    },
    "./oauth": {
      "require": {
        "types": "./dist/strategy/oauth.strategy.d.ts",
        "default": "./dist/strategy/oauth.strategy.cjs"
      },
      "import": {
        "types": "./dist/strategy/oauth.strategy.d.ts",
        "default": "./dist/strategy/oauth.strategy.mjs"
      }
    }
  },
  "keywords": [],
  "dependencies": {
    "@types/jsonwebtoken": "^9.0.6",
    "@tytan-auth/common": "workspace:*",
    "@tytan-auth/provider": "workspace:*",
    "arctic": "^1.9.2",
    "jsonwebtoken": "^9.0.2",
    "pkgroll": "^2.4.2",
    "typescript": "^5.5.4"
  }
}